{
  "homepage": "http://201.1.162.182:3000/home",
  "name": "caopanha",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "mock": "json-server --watch mock/mock-db.json --port 3000 &",
    "start": "node set-env.js && vite",
    "server": "node set-env.js && vite --host 0.0.0.0 --port 3000",
    "stop-mock": "lsof -t -i:3000 | xargs kill -9 || echo 'No mock server running on port 3000'",
    "restart": "yarn stop-mock && yarn mock && yarn start",
    "build": "node set-env.js && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.1",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.13.2",
    "router-dom": "^3.0.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/express": "^5.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^15.2.10",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "main": "index.js",
  "license": "MIT"
}
